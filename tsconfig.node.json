{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": [
    "electron.vite.config.*",
    "src/main/**/*",
    "src/preload/**/*",
    "src/models/**/*",
    "src/db/*",
    "src/renderer/src/**/*",
    "src/types/globals.d.ts"
  ],  "compilerOptions": {
    "composite": true,
    "types": ["electron-vite/node"],
  "baseUrl": ".",
    "paths": {
      "@db/*": ["src/db/*"],
      "@main/*": ["src/main/*"],
      "@renderer/*": ["src/renderer/src/*"],
      "@models/*": ["src/models/*"]
    }
  }
}
